package Day1;

public class Pattern {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		for(int i=1;i<=5;i++)
		{
			
			
			for(int k=0;k<i;k++)
			{
			System.out.print("*");
			}
			///System.out.println("");
		
for(int r=1;r<=5;r++)
	{
	for(int s=0;s<r;s++)
	{
		
	}
		System.out.print("#");
	}
	System.out.println();
	}
	}

}
