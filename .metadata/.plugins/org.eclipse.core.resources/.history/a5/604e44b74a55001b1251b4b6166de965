package Day1;

public class forLoop {

	public static void main(String[] args) {
		
		
		for(int i=1;i<=10;i++)
		{
			//System.out.println(i+4);
		}
		System.out.println("outside of loop");
	
	
		
	
      for(int n=1;n<=50;n++)
      {
    	  
    	  if(n%2 !=0)
    	  {
    	  System.out.println("even  num "+n);
    	  }
      }
    	  
	}
}