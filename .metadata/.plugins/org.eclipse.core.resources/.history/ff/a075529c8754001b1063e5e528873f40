package Day1;

public class ifElseCondition {

	public static void main(String[] args) {
		
		
		/*int gradeA=90;
		int gradeB=70;
		int gradeC=60;*/
			
		int grade=90;
		
		if(grade ==90)
		{
			System.out.println("gradeA");
		}
		else if (grade == 70)
		{
			System.out.println("gradeB");
		}
		else
		{
			System.out.println("gradec");
		}
	}

}
