package Day1;

public class ifElseCondition {

	public static void main(String[] args) {
		
	
		int a=5;
		int b=3;
		
		
			if (a>b)    
			{
			   System.out.println(a);
			   {
				   
			   }
			   else
			   {
				  System.out.println(b); 
			   }
		   
					   
				   }
				
				
				///System.out.println("B is greater "+b);
		      }
			
		
		
	}

}
