package oops;


class Area
{
	int l;
	int b;
	Area(int l,int b)
	{
		this.l=l;
		this.b=b;
	}
}
class square extends Area
{

	square(int l, int b) {
		super(l, b);
		// TODO Auto-generated constructor stub
	}
	
}
class rectangele extends Area
{

	rectangele(int l, int b) {
		super(l, b);
		// TODO Auto-generated constructor stub
	}
	
}

public class Demo extends Area {
	
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Area ar=new Area(20,30);
	}

}
