package Day1;

public class MathmaticOperation {

	
	public int sum(int a,int b)  //return type
	{
		//System.out.println(a+b);
		return a+b;
	}
	
	public int Sub(int a,int b)
	{
		//System.out.println(a-b);
		return a-b;
	}
	
	public void mul(int a,int b)
	{
		System.out.println(a*b);
	}
	
	public void div(int a,int b)
	{
		System.out.println(a/b);
	}
	
	
	
	public static void main(String[] args) {
		
		MathmaticOperation operation=new MathmaticOperation();
		
		int add=operation.sum(20, 10);
		System.out.println(add);
		int subtraction=operation.Sub(20, 10);
		System.out.println(subtraction);
		operation.mul(20, 10);
		operation.div(20, 10);

	}

}
