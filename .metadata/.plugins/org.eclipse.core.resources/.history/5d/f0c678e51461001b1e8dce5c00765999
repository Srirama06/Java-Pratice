package demo03;

public class Bank01 {
	

	
	public static int initialAmount;
	
	
	
	public void deposit(int amount)
	{
		System.out.println(initialAmount+amount);
	}
	public void withdraw(int Amount)
	{
		System.out.println(initialAmount-Amount);
	}
     public void getBalalnce()
     {
    	 System.out.println(initialAmount);
     }

	public static void main(String[] args) {
	
		Bank_1class bank=new Bank_1class();
		bank.deposit(500);
		bank.withdraw(100);
		bank.getBalalnce();
		
		 System.out.println(Bank_1class.initialAmount);
	}
	



}
