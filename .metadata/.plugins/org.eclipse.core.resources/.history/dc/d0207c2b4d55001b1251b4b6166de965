package Day1;

public class ifElseCondition {

	public static void main(String[] args) {
		
	
		int a=5;
		int b=3;
		
		
			if (a>b)  
				
			{
			   System.out.println("A is greater");
			}
			   
			   else
		    {
			   System.out.println("B is greater");
		    
		    }
			
			
		 int Aa=9;
		 int Bb=3;
				 if(Aa>Bb )
				 {
					 System.out.println("Aa is greater");
				 }
				 else
				 {
					 System.out.println("Bb is greater");
				 }
			int aA=100;
			int bB=999;
					if (aA>bB)
					{
						System.out.println("aA is greater");
					}
					else
					{
						System.out.println("bB is greatre");
					}
			int AA=45;
			int BB=35;
			if (AA>BB)
			{
				System.out.println("AA is greater");
			}
			else
			{
				System.out.println("BB is greater");
			}
			
			int x=30;
			int y=35;
			int z=20;
			int big;

			
			if(x>y)
			{
				big=x;
				
				if(big>z)
				{
					System.out.println("x is greater "+big);
				}
				else
					System.out.println("y is greater "+y);
			}
			else
			{
				big=y;
				
				if(big>z)
				{
					System.out.println("y is greater "+big);
				}
				else
					System.out.println("z is greater "+z);
			}
			
			System.out.println("-------------------------------");
			
			
			int i=3;
			
			if(i==0)
			{
				System.out.println("Today is sunday");
			}
			else if (i==1)	
			{
				System.out.println("Today is Monday");
			}
			else if (i==2)	
			{
				System.out.println("Today is Tuesday");
			}
			else if (i==3)	
			{
				System.out.println("Today is Wednesday");
			}
			else if (i==4)	
			{
				System.out.println("Today is Thrusday");
			}
			else if (i==5)	
			{
				System.out.println("Today is Friday");
			}
			else
			{
				System.out.println("Today is Saturday");
			}
	}
}

	              
	            	  
	              
	            	  
	            	  
	            	  
	        
	
				 
					 
						
			   
			 
			